{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended"
  ],
  "plugins": ["@typescript-eslint"],
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "@typescript-eslint/no-unused-vars": "off",
    "prefer-const": "error",
    "no-var": "error",
    "no-unused-vars": "off"
  }
}